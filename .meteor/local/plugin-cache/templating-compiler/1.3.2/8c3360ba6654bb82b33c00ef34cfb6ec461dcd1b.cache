{"head":"","body":"","js":"\nTemplate.__checkName(\"gridControls\");\nTemplate[\"gridControls\"] = new Template(\"Template.gridControls\", (function() {\n  var view = this;\n  return Blaze.If(function() {\n    return Spacebars.dataMustache(view.lookup(\"hasPermission\"), \"createProduct\");\n  }, function() {\n    return [ \"\\n    \", HTML.DIV({\n      class: \"product-grid-controls\"\n    }, \"\\n      \", HTML.LABEL({\n      class: \"like-button hidden\",\n      for: function() {\n        return [ \"select-product-\", Spacebars.mustache(Spacebars.dot(view.lookup(\"product\"), \"_id\")) ];\n      }\n    }, \"\\n        \", HTML.INPUT({\n      type: \"checkbox\",\n      name: \"selectProduct\",\n      value: function() {\n        return Spacebars.mustache(Spacebars.dot(view.lookup(\"product\"), \"_id\"));\n      },\n      id: function() {\n        return [ \"select-product-\", Spacebars.mustache(Spacebars.dot(view.lookup(\"product\"), \"_id\")) ];\n      },\n      checked: function() {\n        return Spacebars.mustache(view.lookup(\"checked\"));\n      }\n    }), \"\\n      \"), \"\\n\\n      \", Blaze.If(function() {\n      return Spacebars.call(Spacebars.dot(view.lookup(\"product\"), \"isDeleted\"));\n    }, function() {\n      return [ \"\\n        \", HTML.SPAN({\n        class: \"badge badge-danger\",\n        \"data-i18n\": \"app.archived\"\n      }, \"\\n          \", HTML.SPAN(\"Archived\"), \"\\n        \"), \"\\n      \" ];\n    }), \"\\n\\n      \", Blaze.If(function() {\n      return Spacebars.call(view.lookup(\"hasChanges\"));\n    }, function() {\n      return [ \"\\n        \", HTML.DIV(Blaze._TemplateWith(function() {\n        return Spacebars.call(view.lookup(\"VisibilityButton\"));\n      }, function() {\n        return Spacebars.include(view.lookupTemplate(\"React\"));\n      })), \"\\n      \" ];\n    }), \"\\n\\n    \"), \"\\n  \" ];\n  });\n}));\n","bodyAttrs":{}}