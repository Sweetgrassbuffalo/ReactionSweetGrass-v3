{"head":"","body":"","js":"\nTemplate.__checkName(\"productImageGallery\");\nTemplate[\"productImageGallery\"] = new Template(\"Template.productImageGallery\", (function() {\n  var view = this;\n  return HTML.DIV({\n    id: \"galleryDropPane\",\n    class: \"galleryDropPane\"\n  }, \"\\n  \", HTML.UL({\n    class: \"gallery\"\n  }, \"\\n    \", Blaze.Each(function() {\n    return Spacebars.call(view.lookup(\"media\"));\n  }, function() {\n    return [ \"\\n      \", Spacebars.include(view.lookupTemplate(\"imageDetail\")), \"\\n    \" ];\n  }, function() {\n    return [ \"\\n     \", HTML.LI(\"\\n        \", HTML.IMG({\n      src: \"/resources/placeholder.gif\",\n      class: \"img-responsive\",\n      id: \"img-upload\"\n    }), \"\\n     \"), \"\\n    \" ];\n  }), \"\\n  \"), \"\\n  \", Spacebars.include(view.lookupTemplate(\"imageUploader\")), \"\\n\");\n}));\n\nTemplate.__checkName(\"imageDetail\");\nTemplate[\"imageDetail\"] = new Template(\"Template.imageDetail\", (function() {\n  var view = this;\n  return HTML.LI({\n    class: \"gallery-image gallery-sortable\",\n    \"data-index\": function() {\n      return Spacebars.mustache(view.lookup(\"_id\"));\n    }\n  }, \"\\n    \", HTML.IMG({\n    src: function() {\n      return Spacebars.mustache(view.lookup(\"url\"), Spacebars.kw({\n        uploading: \"/resources/placeholder.gif\",\n        storing: \"/resources/placeholder.gif\",\n        store: \"large\"\n      }));\n    },\n    class: \"img-responsive\",\n    alt: function() {\n      return Spacebars.mustache(view.lookup(\"title\"));\n    }\n  }), \"\\n    \", Blaze.Unless(function() {\n    return Spacebars.call(Spacebars.dot(view.lookup(\".\"), \"isUploaded\"));\n  }, function() {\n    return [ \"\\n      \", Blaze._TemplateWith(function() {\n      return {\n        bootstrap: Spacebars.call(true)\n      };\n    }, function() {\n      return Spacebars.include(function() {\n        return Spacebars.call(Spacebars.dot(view.lookup(\"FS\"), \"UploadProgressBar\"));\n      });\n    }), \"\\n    \" ];\n  }), \"\\n    \", Blaze.If(function() {\n    return Spacebars.dataMustache(view.lookup(\"hasPermission\"), \"createProduct\");\n  }, function() {\n    return [ \"\\n      \", HTML.DIV({\n      class: \"gallery-tools\"\n    }, \"\\n        \", HTML.A({\n      class: \"image-remove-link\",\n      \"data-index\": function() {\n        return Spacebars.mustache(view.lookup(\"_id\"));\n      }\n    }, HTML.I({\n      class: \"remove-image fa fa-times-circle fa-2x\"\n    })), \"\\n      \"), \"\\n    \" ];\n  }), \"\\n  \");\n}));\n\nTemplate.__checkName(\"imageUploader\");\nTemplate[\"imageUploader\"] = new Template(\"Template.imageUploader\", (function() {\n  var view = this;\n  return Blaze.If(function() {\n    return Spacebars.dataMustache(view.lookup(\"hasPermission\"), \"createProduct\");\n  }, function() {\n    return [ \"\\n  \", HTML.DIV({\n      id: \"dropzone\",\n      class: \"dropzone\"\n    }, \"\\n   \", HTML.INPUT({\n      name: \"files\",\n      id: \"files\",\n      class: \"hidden\",\n      type: \"file\",\n      multiple: \"\"\n    }), \"\\n    \", HTML.DIV({\n      class: \"btn btn-default btn-block\",\n      id: \"btn-upload\",\n      \"data-i18n\": \"productDetail.dropFiles\"\n    }, \"Drop file to upload\"), \"\\n  \"), \"\\n\" ];\n  });\n}));\n","bodyAttrs":{}}